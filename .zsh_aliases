DOTFILES="~/Tools/dots"
VMFOLDER="$HOME/Tools/VMs/"

if [ -f $DOTFILES/.im_aliases ];
	then . $DOTFILES/.im_aliases
fi

# Light tweaks
alias cats='highlight -O ansi --force -n'

# pwn
alias pwninit='pwninit --template-path ~/.config/pwninit-template.py'

# vms
alias archlinux="pushd $VMFOLDER/arch && vagrant ssh && popd"

